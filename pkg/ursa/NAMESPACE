useDynLib(ursa)

importFrom("grDevices", "col2rgb", "colorRampPalette", "rgb", "png",
           "graphics.off", "as.raster")
importFrom("graphics", "abline", "axTicks", "axis", "contour",
           "filled.contour", "hist", "image", "mtext", "plot",
           "polygon", "rect", "strheight", "strwidth", "text", 
